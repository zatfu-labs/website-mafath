<table class="table">
  <thead>
    <tr>
      <th scope="col">NO</th>
      <th scope="col">Nama</th>
      <th scope="col">Kelas</th>
      <th scope="col">Jabatan</th>
    </tr>
  </thead>
  <tbody>
    <% if (users.length === 0) { %>
      <tr>
          <td colspan="4">
              <div class="alert alert-danger fw-bold" role="alert">Data Masih Kosong.</div>
          </td>
      </tr>
  <% }  %>
  <% users.forEach((user, no) => { no++ %>
      <tr>
          <th scope="row"><%= no++ %> </th>
          <td><%= user.nama %> </td>
          <td><%= user.kelas %> </td>
          <td><%= user.hak %> </td>
        </tr>
  <% }) %>
  </tbody>
</table>
